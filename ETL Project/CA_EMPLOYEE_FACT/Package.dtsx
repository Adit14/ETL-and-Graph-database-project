              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT        customers.salesRepEmployeeNumber, customers.customerNumber,
sum(orderdetails.quantityOrdered) as UnitsSoldByEmployee, sum(orderdetails.priceEach) as SalesByEmployee
                         
FROM            customers INNER JOIN
                         employees ON customers.salesRepEmployeeNumber = employees.employeeNumber INNER JOIN
                         orders ON customers.customerNumber = orders.customerNumber INNER JOIN
                         orderdetails ON orders.orderNumber = orderdetails.orderNumber

group by  customers.salesRepEmployeeNumber, customers.customerNumber</property>
